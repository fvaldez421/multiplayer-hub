{
  "env": {
    "browser": true,
		"es2020": true,
		"shared-node-browser": true
  },
  "extends": [
    "airbnb-base",
		"prettier",
		"prettier/react",
    "plugin:react/recommended"
  ],
  "parserOptions": {
		"ecmaVersion": 11,
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
		"react/no-unescaped-entities": 0,
		"import/no-named-as-default": 0,
		"import/no-extraneous-dependencies": 0
	},
  "ignorePatterns": "**/utils/ssrUtils/*.js",
  "settings": {
    "react": {
      "version": "16.13"
    }
  }
}
