{
  "name": "multiplayer-hub",
  "author": "",
  "license": "ISC",
  "version": "1.0.0",
  "description": "Gaming space that hosts various multiplayer games.",
  "homepage": "https://github.com/nad0m/multiplayer-hub#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nad0m/multiplayer-hub.git"
  },
  "bugs": {
    "url": "https://github.com/nad0m/multiplayer-hub/issues"
  },
  "engines": {
    "node": "^10.0.0"
  },
  "main": "index.js",
  "scripts": {
    "lint": "prettier --write eslint --fix src/client",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "cross-env NODE_ENV=development nodemon --inspect src/server --config nodemon.json",
    "prod": "cross-env NODE_ENV=production node dist/server",
    "deploy": "npm run build:all && gcloud app deploy -q --version=script-deploy --no-promote",
    "build:all": "npm run build:client && npm run build:server",
    "build:client": "rimraf build/ && cross-env NODE_ENV=production webpack --config=src/config/webpack.config.prod.js",
    "build:server": "rimraf dist/ && node scripts/copyServer && cross-env NODE_ENV=production babel src/ --out-dir dist/"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "@babel/polyfill": "^7.10.1",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "apollo-link-http": "^1.5.17",
    "apollo-link-schema": "^1.2.5",
    "apollo-link-ws": "^1.0.20",
    "apollo-server-express": "^2.14.2",
    "chalk": "^4.1.0",
    "core-js": "^3.6.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "firebase": "^7.15.1",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.10.3",
    "graphql-tools": "^6.0.8",
    "isomorphic-fetch": "^2.2.1",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-apollo": "^3.1.5",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.1.1",
    "styled-components-breakpoint": "^3.0.0-preview.20",
    "styled-icons": "^10.2.1",
    "subscriptions-transport-ws": "^0.9.16",
    "ws": "^7.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.10.1",
    "@babel/register": "^7.5.5",
    "babel-loader": "^8.1.0",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "babel-plugin-styled-components": "^1.10.7",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "eslint": "7.4.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.0",
    "husky": "^4.2.5",
    "mini-css-extract-plugin": "^0.9.0",
    "nodemon": "^2.0.4",
    "prettier": "2.0.5",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^3.0.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
