  "license": "ISC",
  "version": "1.0.0",
  "description": "Gaming space that hosts various multiplayer games.",
  "homepage": "https://github.com/nad0m/multiplayer-hub#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nad0m/multiplayer-hub.git"
  },
  "bugs": {
    "url": "https://github.com/nad0m/multiplayer-hub/issues"
  },
  "engines": {
    "node": "^10.0.0"
  },
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon src/server/development",
    "prod": "node dist/server/production",
    "deploy": "npm run build:all && gcloud app deploy --version=script-deploy -q",
    "build:all": "npm run build:client && npm run build:server",
    "build:client": "rm -rf build/ && webpack --config=src/config/webpack.config.prod.js",
    "build:server": "rm -rf dist/ && node scripts/copyServer && babel src/ --out-dir dist/"
  },
  "dependencies": {
    "@babel/polyfill": "^7.10.1",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-http": "^1.5.17",
    "apollo-link-schema": "^1.2.5",
    "apollo-server-express": "^2.14.2",
    "core-js": "^3.6.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "firebase": "^7.15.1",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.10.3",
    "graphql-tools": "^6.0.8",
    "react": "^16.13.1",
    "react-apollo": "^3.1.5",
    "react-dom": "^16.13.1",
    "styled-components": "^5.1.1",
    "styled-components-breakpoint": "^3.0.0-preview.20",
    "styled-icons": "^10.2.1",
    "universal-react-apollo": "^1.0.22"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.10.1",
    "@babel/register": "^7.5.5",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "chalk": "^4.1.0",
    "cross-env": "^7.0.2",
    "mini-css-extract-plugin": "^0.9.0",
    "nodemon": "^2.0.4",
    "terser-webpack-plugin": "^3.0.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-manifest-plugin": "^2.2.0"
  }
